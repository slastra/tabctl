{
  "description": "Control your browser's tabs from command line with tabctl",
  "manifest_version": 2,
  "name": "TabCtl",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
      "128": "tabctl-icon-128x128.png"
  },
  "permissions": ["nativeMessaging", "tabs", "activeTab", "<all_urls>"]
}
